  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "http-errors": "~1.6.2",
    "mariadb": "^2.0.3",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "sequelize": "^5.2.3",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "nodemon": "^1.18.10"
